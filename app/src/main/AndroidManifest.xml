<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.davis.uitrapulltorefresh.demo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activity.RentalsStyleActivity"></activity>
        <activity android:name=".activity.MaterialStyleActivity"></activity>
        <activity android:name=".activity.UsingPointActivity"></activity>
        <activity android:name=".activity.StoreHouseStringActivity"></activity>
        <activity android:name=".activity.StoreHouseArrayActivity"></activity>
        <activity android:name=".activity.AutoRefreshActivity"></activity>
        <activity android:name=".activity.DrapDownRefreshActivity"></activity>
        <activity android:name=".activity.ReleaseRefreshActivity"></activity>
        <activity android:name=".activity.WebViewActivity"></activity>
        <activity android:name=".activity.ScrollViewActivity"></activity>
        <activity android:name=".activity.ListViewActivity"></activity>
        <activity android:name=".activity.GridViewActivity"></activity>
    </application>

</manifest>